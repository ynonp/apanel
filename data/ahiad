<VirtualHost *:80>
	ServerAdmin webmaster@localhost

        ServerName ronenkook.co.il
        ServerAlias www.ronenkook.co.il

        DocumentRoot /home/ahiad/web/ronen/

        <Directory /Users/ynonperek/test>
		Order Allow,Deny
		Allow from all
        </Directory>

        JkMount  /* ajp13_worker

        RewriteEngine On
        RewriteRule ^$ /Ronen [PT]
        RewriteRule ^/(.*) /Ronen/$1 [PT]

</VirtualHost>

<VirtualHost *:80>
        ServerAdmin webmaster@localhost

        ServerName aquafml.com
        ServerAlias www.aquafml.com

        DocumentRoot /home/ahiad/web/aquafml/

        <Directory /home/ahiad/web/aquafml>
#               Options FollowSymLinks
#               AllowOverride None
        Order Allow,Deny
        Allow from all
        </Directory>

# Send everything for context /examples to worker named worker1 (ajp13)
        JkMount  /* ajp13_worker

        RewriteEngine On
        RewriteRule ^$ /Aquafml [PT]
        RewriteRule ^/(.*) /Aquafml/$1 [PT]

</VirtualHost>


